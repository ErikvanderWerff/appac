Package: appac
Type: Package
Title: Atmospheric Pressure Peak Area Correction
Version: 0.2.0
Authors@R: c(
    person(given = "Rüdiger", family = "Forster", role = c("aut","cre"), 
    email = "ruediger.forster0212@gmail.com", 
    comment = c(ORCID = "0009-0006-4851-7430")),
